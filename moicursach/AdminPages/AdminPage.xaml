<Page x:Class="moicursach.AdminPage"
      xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
      xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
      xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
      xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
      xmlns:local="clr-namespace:moicursach"
      mc:Ignorable="d" 
      d:DesignHeight="450" d:DesignWidth="800"
      Title="AdminPage">
    <StackPanel>
        <Button x:Name="Privods" Height="30" Width="130" HorizontalAlignment="Center" VerticalAlignment="Center" Click="Privods_Click">Привода</Button>
        <Button x:Name="Oils" Height="30" Width="130" HorizontalAlignment="Center" VerticalAlignment="Center" Click="Oils_Click">Виды топлива</Button>
        <Button x:Name="Types" Height="30" Width="130" HorizontalAlignment="Center" VerticalAlignment="Center" Click="Types_Click">Типы</Button>
        <Button x:Name="Brands" Height="30" Width="130" HorizontalAlignment="Center" VerticalAlignment="Center" Click="Brands_Click">Марки</Button>
        <Button x:Name="KPP_Types" Height="30" Width="130" HorizontalAlignment="Center" VerticalAlignment="Center" Click="KPP_Click">КПП</Button>
        <Button x:Name="Bodies_Types" Height="30" Width="130" HorizontalAlignment="Center" VerticalAlignment="Center" Click="Bodies_Click">Кузова</Button>
    </StackPanel>
</Page>
